    "@daocasino/dc-ethereum-utils": "^0.2.5",
    "@daocasino/dc-logging": "^0.2.5",
    "@daocasino/dc-messaging": "^0.2.5",
    "@daocasino/dc-statistics-client": "^0.2.5",
    "@daocasino/dc-webapi": "^0.2.5",
    "chai": "^4.1.2",
    "commander": "^2.19.0",
    "mocha": "^3.2.0",
    "node-fetch": "^2.2.0",
    "nodemon": "^1.18.5",
    "rimraf": "2.6.2",
    "raven": "^2.6.4"
